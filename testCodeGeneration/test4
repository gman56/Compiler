max2(a : int, b : int) : int
{
        if (a > b)
                then return a
                else return b
}

max4(a:int, b:int, c:int, d:int) : int
{
        t1 : int
        t2 : int
        t3 : int
        t1 := max2(a,b)
        t2 := max2(c,d)
        t3 := max2(t1,t2)
        return t3
}


main() : int
{
        i : int
        j : int
        m : int
        i := 10
        j := 30
        m := max4(i, 20, j, 40)
        print m
        m := max4(40, j, 20, i)
        print m
        m := max4(1, j, i, i)
        print m
        m := max4(i, i, i, i)
        print m
        return 0
}
