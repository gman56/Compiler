max(a : int, b : int) : int
{
        if (a > b)
                then return a
                else return b
}


main() : int
{
        i : int
        A : arrayof 10 int
        i := 0
        while (i < 10) do
        {
                A[i] := i
                i := i + 1
                A[i] := -i
                i := i + 1
        }
        i := 0
        while (i < 9) do
        {
                print max(A[i], A[i+1])
                i := i + 1
        }
        return 0
}
