main() : int
{
        i : int
        A : arrayof 10 int
        A[1] := 1
        i := 2
        while (i < 10) do
        {
                A[i] := A[i-1] + i
                i := i + 1
        }
        i := 1
        while (i < 10) do
        {
                print A[i]
                i := i + 2
        }
        return 0
}